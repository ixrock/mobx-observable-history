{
  "name": "mobx-observable-history",
  "version": "1.0.3",
  "description": "History.js wrapper with observable location and reactive URLSearchParams",
  "repository": "https://github.com/ixrock/mobx-observable-history",
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsc",
    "test": "jest --verbose",
    "test-watch": "jest --watch",
    "pack": "npm pack"
  },
  "keywords": [
    "mobx",
    "observable",
    "history",
    "location",
    "search-params",
    "url-search-params",
    "url-search-params-extended"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "lib/*.*(js|d.ts)",
    "index.d.ts"
  ],
  "dependencies": {
    "@types/history": "^4.7.3",
    "history": "^4.10.1",
    "mobx": "^5.15.4"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.3",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "author": "ixrock",
  "license": "MIT"
}
